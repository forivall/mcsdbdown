{
  "name": "$NAME",
  "version": "0.0.0",
  "description": "$DESCRIPTION",
  "private": true,
  "main": "$NAME.js",
  "scripts": {
    "lint": "xo src/**",
    "inspect": "jsinspect",
    "pretest": "npm run lint && npm run inspect",
    "test": "ava",
    "test-cov": "nyc ava"
  },
  "engines": {
    "node": ">=0.10.25 <0.12",
    "express": "3.5.1"
  },
  "oracleMobile": {
    "dependencies": {
      "apis": {},
      "connectors": {}
    }
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.11.0",
    "jsinspect": "^0.7.0",
    "nyc": "^5.5.0",
    "xo": "^0.12.1"
  },
  "xo": {
    "space": 2,
    "rules": {
      "curly": [
        2,
        "multi-line",
        "consistent"
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "eqeqeq": [
        2,
        "smart"
      ],
      "no-eq-null": [
        0
      ],
      "no-use-before-define": [
        1
      ]
    },
    "overrides": [
      {
        "files": "test/**",
        "esnext": true,
        "semicolon": false
      }
    ]
  }
}
