{
  "name": "mcsdbdown",
  "version": "1.0.0",
  "description": "A backend for levelup that uses MCS's Database Access API",
  "main": "index.js",
  "scripts": {
    "lint": "xo src/**",
    "inspect": "jsinspect",
    "pretest": "npm run lint && npm run inspect",
    "test": "ava",
    "test-cov": "nyc ava"
  },
  "engines": {
    "node": ">=0.10.25 <0.12",
    "express": "3.5.1"
  },
  "oracleMobile": {
    "dependencies": {
      "apis": {},
      "connectors": {}
    }
  },
  "dependencies": {
    "abstract-leveldown": "^2.6.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "jsinspect": "^0.8.0",
    "nyc": "^15.1.0",
    "xo": "^0.15.1"
  },
  "xo": {
    "space": 2,
    "rules": {
      "linebreak-style": 0,
      "curly": [
        2,
        "multi-line",
        "consistent"
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "eqeqeq": [
        2,
        "smart"
      ],
      "no-eq-null": [
        0
      ],
      "no-use-before-define": [
        1
      ]
    },
    "overrides": [
      {
        "files": "test/**",
        "esnext": true,
        "semicolon": false
      }
    ]
  }
}
